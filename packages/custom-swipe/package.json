  },
  "dependencies": {
    "swipe-core-provider": "^1.0.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/preset-typescript": "^7.23.3",
    "typescript": "^5.3.3"
  }
